{
  "name": "@potw/type-utils",
  "version": "1.0.14",
  "dependencies": {
    "tslib": "^2.3.0",
    "zod": "^3.24.1"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
